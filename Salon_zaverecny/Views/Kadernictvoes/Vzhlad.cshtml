
@{
    ViewData["Title"] = "Vzhlad";

}



<table class="table table-hover">
    <thead>
        <tr>
            <th scope="col">Ponuka</th>
            <th scope="col">Cena</th>

        </tr>
    </thead>
    <tbody>
        <tr>
            <th scope="row">Dámsky strih</th>
            <td>5-20€</td>

        </tr>
        <tr>
            <th scope="row">Pánsky strih</th>
            <td>5-10€</td>

        </tr>
        <tr>
            <th scope="row">Detský strih</th>
            <td colspan="2">5-10€</td>

        </tr>

        <tr>
            <th scope="row">Farbenie</th>
            <td>20-50€</td>

        </tr>

        <tr>
            <th scope="row">Melír</th>
            <td>20-50€</td>
        </tr>


        <tr>
            <th scope="row">Svadobný účes</th>
            <td>30-50€</td>
        </tr>


        <tr>
            <th scope="row">Spoločenský účes</th>
            <td>10-50€</td>
        </tr>

        <tr>
            <th scope="row">Predlžovanie vlasov</th>
            <td>50-100€</td>
        </tr>

        <tr>
            <th scope="row">Zahusťovanie vlasov</th>
            <td>50- 100€</td>
        </tr>


    </tbody>
</table>

<a typeof="button" asp-area="" asp-controller="Kadernictvoes" asp-action="Formular" type="submit" class="btn btn-primary"> Objednať </a>
